{
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "numReplicas": 1,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "healthcheck": {
      "port": 8080,
      "path": "/",
      "interval": 10000,
      "timeout": 2000,
      "retries": 3
    }
  }
}
